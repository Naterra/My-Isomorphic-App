{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project ",
  "main": "index.js",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node build_server/bundle.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "webpack --mode development --config webpack.client.js --watch ",
    "server": "nodemon --exec   babel-node server/index.js",
    "prod:compile_server": "babel server -d build_server --presets es2017,stage-1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false  npm install && npm run prod:compile_server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "css-loader": "^0.28.11",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "lodash": "4.17.4",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "query-string": "^6.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sass-loader": "^6.0.7",
    "serialize-javascript": "1.4.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack-isomorphic-tools": "^3.0.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.1"
  }
}
